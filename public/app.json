{
    "runtime": {
        "arguments": "--v=1 --inspect",
        "version": "alpha"
    },
    "platform": {
        "uuid": "AdapterInteropTest",
        "applicationIcon": "https://openfin.github.io/golden-prototype/favicon.ico",
        "autoShow": false,
        "providerUrl": "${APP_ROOT_URL}/provider.html",
        "defaultWindowOptions": {
            "url": "${APP_ROOT_URL}/platform-window.html",
            "contextMenu": true,
            "defaultWidth": 600,
            "defaultHeight": 600,
            "defaultLeft": 0,
            "defaultTop": 0,
            "saveWindowState": false,
            "backgroundThrottling": true,
            "minHeight": 445,
            "minWidth": 354
        },
        "defaultViewOptions": {
        },
        "commands": [
            {
                "command": "stack.nextTab",
                "keys": "Ctrl+Tab"
            }
        ]
    },
    "snapshot": {
        "windows": [
            {
                "layout": {
                    "content": [
                        {
                            "type": "stack",
                            "id": "no-drop-target",
                            "content": [
                                {
                                    "type": "component",
                                    "componentName": "view",
                                    "componentState": {
                                        "processAffinity": "ps_1",
                                        "url": "${APP_ROOT_URL}/platform-view.html",
                                        "name": "adapter-testing-view"
                                    }
                                }
                            ]
                        }
                    ]
                }
            }
        ]
    }
}
