{
    "name": "virtual-app-proxy",
    "scripts": {
        "copy-assets": "copy public\\app.json out && copy public\\*.css out",
        "clean": "rimraf ./out",
        "build": "webpack --mode=development && npm run copy-assets",
        "deploy": "aws s3 cp --recursive ./out s3://cdn.openfin.co/virtual-app/proxy"
    },
    "devDependencies": {
        "css-loader": "^6.2.0",
        "html-webpack-plugin": "5.3.2",
        "openfin-adapter": "20.63.5",
        "rimraf": "3.0.2",
        "style-loader": "^3.2.1",
        "ts-loader": "9.2.3",
        "typescript": "4.3.5",
        "webpack": "5.44.0",
        "webpack-cli": "4.7.2"
    }
}
